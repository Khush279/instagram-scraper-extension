{
    "manifest_version": 3,
    "name": "Simple Instagram Scraper",
    "version": "1.0",
    "description": "Scrapes usernames and captions from Instagram feed and allows searching by caption keywords.",
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://www.instagram.com/*"],
        "js": ["content.js"]
      }
    ]
  }